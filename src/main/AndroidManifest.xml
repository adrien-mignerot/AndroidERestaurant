<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.isen.mignerot.guinguetteclickandcollect">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidERestaurant"
        android:usesCleartextTraffic="true">
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.OrderResultActivity"></activity>
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.LoginActivity" />
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.RegisterActivity" />
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.BasketActivity" />
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.DetailActivity" />
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.CategoryActivity" />
        <activity android:name="fr.isen.mignerot.guinguetteclickandcollect.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>